prefix=@prefix@
libdir=@libdir@
includedir=@includedir@

Name: puTools
Description: The puTools Library
Version: @VERSION@
Requires: puCtools >= 5.0.0
Libs: -fPIC -L${libdir} -lpuTools @CURL_LDFLAGS@ @BOOST_LDFLAGS@ @CURL_LIBS@ @BOOST_DATE_TIME_LIBS@
Cflags: -I${includedir} -fPIC @CURL_CPPFLAGS@ @BOOST_CPPFLAGS@
